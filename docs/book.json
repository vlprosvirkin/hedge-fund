{
  "title": "Hedge Fund AI Trading System",
  "description": "Complete documentation for the AI-powered cryptocurrency trading system",
  "author": "Hedge Fund Team",
  "language": "en",
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md"
  },
  "plugins": [
    "expandable-chapters",
    "search",
    "copy-code-button",
    "theme-default",
    "highlight",
    "sharing",
    "fontsettings",
    "livereload"
  ],
  "pluginsConfig": {
    "theme-default": {
      "showLevel": true
    },
    "search": {
      "maxIndexSize": 100000,
      "ignoreCase": true,
      "maxSearchResults": 20
    },
    "sharing": {
      "facebook": true,
      "twitter": true,
      "google": false,
      "weibo": false,
      "instapaper": false,
      "vk": false,
      "all": [
        "facebook", "twitter", "google", "weibo", "instapaper"
      ]
    },
    "fontsettings": {
      "theme": "white",
      "family": "sans",
      "size": 2
    },
    "highlight": {
      "theme": "github"
    }
  }
}
